<script>
	function toggleBibtexno_demo_tool_layout(parameter) {
		var x= document.getElementById('cite_no_demo_tool_layout');
		if (x.style.display === 'none') {
			x.style.display = 'block';
		} else {
			x.style.display = 'none';
		}
	}
</script>

